{
  "name": "category",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": false
    },
    "description": {
      "type": "string",
      "required": false
    },
    "thumbnails": {
      "type": "string",
      "required": false
    },
    "display": {
      "type": "boolean",
      "required": false
    },
    "items": {
      "type": "array",
      "required": false
    },
    "created": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    },
    "modified": {
      "type": "date",
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "categoryChild": {
      "type": "hasMany",
      "model": "categoryChild",
      "foreignKey": "categoryId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "super",
      "permission": "ALLOW",
      "property": "*"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW",
      "property": "*"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
